{
  "name": "aprexis-api-ui",
  "version": "0.1.0",
  "private": true,
  "respository": {
    "type": "git",
    "url": "git+git@github.com:Aprexis/aprexis-api-ui.git"
  },
  "author": "Aprexis",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^4.6.0",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.3",
    "file-saver": "^2.0.5",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-csv": "^2.0.3",
    "react-day-picker": "^7.4.8",
    "react-dom": "^17.0.1",
    "react-idle-timer": "^4.5.6",
    "react-numeric-input": "^2.2.3",
    "react-router-dom": "^5.2.0",
    "react-router-last-location": "^2.0.1",
    "react-scripts": "4.0.3",
    "react-select": "4.3.0",
    "react-spinners": "^0.10.6",
    "react-tooltip": "^4.2.15",
    "reactstrap": "^8.9.0",
    "sass": "^1.34.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "PORT=3500 REACT_APP_APREXIS_API=http://localhost:3250 REACT_APP_APREXIS_KEY=aprexis-storage-secret-key react-scripts start",
    "start-docker": "PORT=3500 REACT_APP_APREXIS_KEY=aprexis-storage-secret-key react-scripts start",
    "build": "react-scripts build && rm -rf dist && mkdir dist && mv build dist/ui",
    "build:staging": "REACT_APP_APREXIS_API=http://staging.aprexis.com RAILS_RELATIVE_URL_ROOT=/api yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": "off",
      "default-case-last": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
